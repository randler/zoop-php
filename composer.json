{
    "name": "randler/lib-zoop-php",
    "description": "zoop PHP Library",
    "type": "lib",
    "keywords": [
      "zoop",
      "gateway",
      "api"
    ],
    "license": "MIT",
    "require": {
      "php": ">=5.6.0",
      "guzzlehttp/guzzle": "^6.4"
    },
    "require-dev": {
      "phpunit/phpunit": "7.0.0",
      "squizlabs/php_codesniffer": "^3.3"
    },
    "scripts": {
      "test": [
        "@composer install",
        "vendor/bin/phpunit"
      ]
    },
    "autoload": {
      "psr-4": {"Zoop\\": "src/"}
    },
    "autoload-dev": {
      "psr-4": {
        "Zoop\\Test\\": "tests/unit"
      }
    }
  }
